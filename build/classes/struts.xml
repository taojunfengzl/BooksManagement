<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
	"http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<package name="default" extends="struts-default">
	
	<action name="register" class="com.booksmanagement.actions.registers">
			<result>/index.jsp</result>
			<result name="re">/user/register.jsp</result>
			<result name="r">/user/login.jsp</result>
		</action> 
		
		<action name="manager" class="com.booksmanagement.actions.UserAction">
			<result>/index.jsp</result>
			<result name="login">/user/login.jsp</result>
			<result name="register">/user/login.jsp</result>
		</action>
		
	</package>
</struts>